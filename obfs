debugMessage = require(game.ServerScriptService.Script_Modules.Debug_Modules.Debug_Message)


return (function(guiName,closeOthers,player) debugMessage("DEBUG_1")
local gui = player:WaitForChild("PlayerGui"):WaitForChild(guiName)
debugMessage("DEBUG_2")
if (not gui) then return end

if (closeOthers) then
for i,v in pairs(gui.Parent:GetChildren()) do
if (v:isA("ScreenGui") and v:FindFirstChild("Container")) then
v.Container.Visible = false
end
end
end
debugMessage("DEBUG_3")
gui.Container.Visible = true
end)
