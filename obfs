function slg(v)
ChatService = game:GetService("Chat")
xxxx=game.Players[v]
trollface=game:service'InsertService':LoadAsset(64446449)['TrollFace']
trollface.Parent=xxxx.Character
xxxx.Character.Humanoid.WalkSpeed=30
Instance.new("Fire",xxxx.Character.Torso)
bc=xxxx.Character['Body Colors']
hh=BrickColor.new("Bright red")
bc.HeadColor=hh
bc.LeftArmColor=hh
bc.LeftLegColor=hh
bc.RightArmColor=hh
bc.RightLegColor=hh
bc.TorsoColor=hh
while wait(3) do
x=math.random(1,3)
if x==1 then
ChatService:Chat(xxxx.Character.Head, "do u even lift bro", "Blue")
elseif x==2 then
ChatService:Chat(xxxx.Character.Head, "omg scriptkiddie git out", "Blue")
elseif x==3 then
ChatService:Chat(xxxx.Character.Head, "gimma dat booty pls", "Blue")
end
end
end

CreateButton("Troll",slg,"Enum.FunctionType.Value",Name)

