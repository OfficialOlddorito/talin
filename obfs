-- Script / Function / Test Guis --
function GiveStyle(idk,ye)
if idk:IsA'TextButton' or idk:IsA'TextLabel' or idk:IsA'TextBox' then
idk.Font=Enum.Font.ArialBold
idk.TextStrokeTransparency=0.8
idk.Font=Enum.Font.ArialBold
idk.TextScaled=true
idk.TextColor3=DefaultColor2
end
idk.BorderSizePixel=0
if ye==nil then
idk.BackgroundColor3=DefaultColor
end
end
WelcomeLabel=Instance.new("TextLabel",Gui)
WelcomeLabel.Text="LeGui"
WelcomeLabel.Size=UDim2.new(1,0,0.1,0)
WelcomeLabel.ZIndex=2
GiveStyle(WelcomeLabel)
Shutdown=Instance.new("TextButton",Gui)
Shutdown.Text="Shutdown"
Shutdown.Size=UDim2.new(0.1,0,0.05,0)
Shutdown.Position=CurrentPos
Shutdown.Name="Shutdown"
GiveStyle(Shutdown)
CurrentPos=CurrentPos+ButtonAdded
function ConfirmationBox(func,ztext) -- A Confirmation, used as CB(Function[IfAccepted],ConfirmText)
Box=Instance.new("Frame",Gui)
Box.Name="ConfirmBox"
Box.ZIndex=5
Box.Position=UDim2.new(-1,0,0.2,0)
Box.Size=UDim2.new(0.8,0,0.5,0)
GiveStyle(Box)
BoxText=Instance.new("TextLabel",Box)
BoxText.ZIndex=6
BoxText.Text=ztext
GiveStyle(BoxText)
BoxText.Size=UDim2.new(1,0,0.3,0)
BoxText.TextYAlignment=Enum.TextYAlignment.Top
-- Accept Button --
AcceptButton=Instance.new("TextButton",Box)
AcceptButton.Size=UDim2.new(0.2,0,0.1,0)
AcceptButton.ZIndex=7
GiveStyle(AcceptButton)
wait(0.2)
AcceptButton.BackgroundColor3=Color3.new(0, 270, 0)
AcceptButton.Text="Yes"
AcceptButton.Position=UDim2.new(0.15,0,0.8,0)
-- Decline Button --
DeclineButton=Instance.new("TextButton",Box)
DeclineButton.Size=UDim2.new(0.2,0,0.1,0)
DeclineButton.ZIndex=7
GiveStyle(DeclineButton,"s")
DeclineButton.BackgroundColor3=Color3.new(170, 0, 0)
DeclineButton.Text="No"
DeclineButton.Position=UDim2.new(0.60,0,0.8,0)
Box:TweenPosition(UDim2.new(0.1,0,0.2,0))
AcceptButton.MouseButton1Down:connect(function()
func()
Box:Remove''
end)
DeclineButton.MouseButton1Down:connect(function()
Box:Remove''
end)
end

Character.Humanoid.Died:connect(function()
a:Remove''
end)
