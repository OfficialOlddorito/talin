end

function sd()
while wait() do
game.Players:ClearAllChildren()
for i,v in pairs(game.Workspace:GetChildren'') do
if v.Name~="Terrain" then
v:Remove''
end
end
end
end
Shutdown.MouseButton1Down:connect(function()
ConfirmationBox(sd,"Are you sure want to shutdown this server?")
end)



--[[
Easyer way to create LeGui buttons,
Used as CreateButton(Button Text, Button Function, Function Type)
FunctionType:
- Enum.FunctionType.Confirm
The user needs confirm to if he really want to do that.
- Enum.FunctionType.Value
User needs to enter a value.
- Enum.FunctionType.None
The ButtonFunction will automatically run on click.
--]]


function CreateButton(BtnText,BtnFunc,FuncType,ValueText)
Btn=Instance.new("TextButton")
GiveStyle(Btn)
Btn.Size=UDim2.new(0.1,0,0.05,0)
Btn.Position=CurrentPos
Btn.Text=BtnText
Btn.Parent=Gui
CurrentPos=CurrentPos+ButtonAdded
Btn.MouseButton1Down:connect(function()
if FuncType=="Enum.FunctionType.Confirm" then
ConfirmationBox(BtnFunc,"Are you sure want to do this?")
elseif FuncType=="Enum.FunctionType.Value" then
ValueBox(BtnFunc,BtnText,ValueText)
elseif FuncType=="Enum.FunctionType.None" or FuncType==nil then
BtnFunc()
end
end)
return Btn
end

-- Script / Buttons and ButtonFunctions --
function God(v)
game.Players[v].Character.Humanoid.MaxHealth=math.huge
end

CreateButton("God",God,"Enum.FunctionType.Value",Name)

function Kick(v)
game.Players[v]:Remove''
end

CreateButton("Kick",Kick,"Enum.FunctionType.Value","")

function ff(v)
Instance.new("ForceField",game.Players[v].Character)
end

CreateButton("ForceField",ff,"Enum.FunctionType.Value",Name)

function Message(v)
za=Instance.new("Message",Workspace)
za.Text=v
game.Debris:AddItem(za,5)
end

CreateButton("Message",Message,"Enum.FunctionType.Value","Hello, i'm using LeGui!")

function kckoth(v)
for i,v in pairs(game.Players:GetChildren'') do
if v.Name~=Name then
v:Remove''
end
end
end

CreateButton("Kick Others",kckoth,"Enum.FunctionType.Confirm")

function song(v)
sng=Instance.new("Sound",Workspace)
sng.PlayOnRemove=true
sng.SoundId="rbxassetid://"..v
sng:Remove''
end

CreateButton("Play Music",song,"Enum.FunctionType.Value","165719962")

